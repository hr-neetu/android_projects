<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/apiView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.264" />

    <TextView
        android:id="@+id/apiView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t2"
        app:layout_constraintTop_toBottomOf="@+id/apiView1"
        app:layout_constraintLeft_toLeftOf="parent"
       />

    <TextView
        android:id="@+id/apiView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t3"
        app:layout_constraintTop_toBottomOf="@+id/apiView2"
        app:layout_constraintLeft_toLeftOf="parent"
       />

    <TextView
        android:id="@+id/apiView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t4"
        app:layout_constraintTop_toBottomOf="@+id/apiView3"
        app:layout_constraintLeft_toLeftOf="parent"
       />

    <TextView
        android:id="@+id/apiView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t5"
        app:layout_constraintTop_toBottomOf="@+id/apiView4"
        app:layout_constraintLeft_toLeftOf="parent"
         />

    <TextView
        android:id="@+id/apiView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t6"
        app:layout_constraintTop_toBottomOf="@+id/apiView5"
        app:layout_constraintLeft_toLeftOf="parent"
         />

    <TextView
        android:id="@+id/apiView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/t7"
        android:autoLink="all"
        app:layout_constraintTop_toBottomOf="@+id/apiView6"
        app:layout_constraintLeft_toLeftOf="parent"
         />
</androidx.constraintlayout.widget.ConstraintLayout>